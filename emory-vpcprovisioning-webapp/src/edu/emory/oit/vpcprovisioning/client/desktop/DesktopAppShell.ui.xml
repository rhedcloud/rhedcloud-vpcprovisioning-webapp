<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="DesktopStyles.css" />
	
	
	<g:LayoutPanel ui:field='layoutPanel' width="100%" height="100%">
		<!-- header -->
		<g:layer top='0px' height='5.0em' left='0px' right='0px'>
			<g:HorizontalPanel ui:field="linksPanel" addStyleNames="{style.linksPanel}" width="100%" height="100%">
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel>
				<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel ui:field='logoTitleBar' title="Home">
						<div ui:field='logoElem'><img src="images/emory-aws-logos.png"/></div>
					</g:HTMLPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel ui:field='releaseInfoPanel'>
						<div ui:field='releaseInfoElem'/>
					</g:HTMLPanel>
				</g:cell>
			</g:VerticalPanel>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
			<g:HorizontalPanel ui:field="generalInfoPanel" width="100%">
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel title="Security Notifications" ui:field='notificationsHTML' styleName="{style.linksMenuHeader}">
						<div ui:field='notificationsElem'><img src="images/bell-512.png" width="24" height="24"/></div>
					</g:HTMLPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel addStyleNames="{style.linksPanelLink}" ui:field='productsTitleBar' title="Emory AWS Services">
						<div ui:field='productsElem'>Products  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/></div>
					</g:HTMLPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<!-- 
					<g:HTMLPanel addStyleNames="{style.linksPanelLink}" ui:field='documentationTitleBar'>
						<div ui:field='documentationElem'>Documentation</div>
					</g:HTMLPanel>
					 -->
					<g:MenuBar ui:field="documentationMenuBar" autoOpen="false" animationEnabled="true" styleName="{style.linksMenu}">
						<g:MenuItem ui:field="documentationMenu" styleName="{style.linksMenuHeader}">Documentation  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="emoryKbItem" debugId="emoryKbItem" title="Search Emory Research Service Documentation" styleName="{style.linksMenuItem}"><a target="_blank" href="https://emory.service-now.com/sp?id=kb_view2">Emory Knowledge Base</a></g:MenuItem>
								<g:MenuItem ui:field="awsDocumentationItem" debugId="awsDocumentationItem" title="AWS Documentation" styleName="{style.linksMenuItem}"><a target="_blank" href="https://aws.amazon.com/documentation/">AWS Documentation</a></g:MenuItem>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:cell>
			</g:HorizontalPanel>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:HorizontalPanel>
				<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel addStyleNames="{style.linksPanelLink}" ui:field='userNameTitleBar'>
						<div ui:field='userNameElem'>Very Very Long User Name</div>
					</g:HTMLPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:MenuBar ui:field="supportMenuBar" autoOpen="false" animationEnabled="true" styleName="{style.linksMenu}">
						<g:MenuItem ui:field="resourcesMenu" styleName="{style.linksMenuHeader}">Support  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="awsItem" debugId="awsItem" title="AWS Enterprise Support" styleName="{style.linksMenuItem}"><a target="_blank" href="https://aws.amazon.com/premiumsupport/enterprise-support/">AWS Enterprise Support</a></g:MenuItem>
								<g:MenuItem ui:field="emoryAwsItem" debugId="emoryAwsItem" title="Emory AWS Support" styleName="{style.linksMenuItem}"><a target="_blank" href="https://emory.service-now.com/sp?id=sc_cat_item&amp;sys_id=58e901fd983a4500593bf372f0e9a92e">Emory AWS Support</a></g:MenuItem>
								<g:MenuItem styleName="{style.menuItemSeparator}">
									<div class="menuSeparatorInner"></div>
								</g:MenuItem> 
								<g:MenuItem ui:field="reportProblemItem" title="Report a Problem" styleName="{style.linksMenuItem}"> Report a Problem
									<g:MenuBar vertical="true">
										<g:MenuItem ui:field="haveAnAccountItem" debugId="haveAnAccountItem" title="AWS Enterprise Support" styleName="{style.linksMenuItem}"><a target="_blank" href="https://aws.amazon.com/premiumsupport/enterprise-support/">I have an account</a></g:MenuItem>
										<g:MenuItem ui:field="doNotHaveAnAccountItem" debugId="doNotHaveAnAccountItem" title="AWS Enterprise Support" styleName="{style.linksMenuItem}"><a target="_blank" href="https://aws.amazon.com/premiumsupport/enterprise-support/">I DO NOT have an account</a></g:MenuItem>
									</g:MenuBar>
								</g:MenuItem>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:cell>
			</g:HorizontalPanel>
			</g:cell>
			</g:HorizontalPanel>
		</g:layer>
		
		<!-- Content -->
		<g:layer top='7.5em' bottom='0px' left='0px' right='0px'>
			<g:ScrollPanel width="100%" height="99%">
			<g:HorizontalPanel width="100%">
				<g:cell horizontalAlignment="ALIGN_LEFT" width="1%"><g:HTML></g:HTML></g:cell>
				<g:cell horizontalAlignment="ALIGN_CENTER" width="98%">
					<g:VerticalPanel ui:field="appShellPanel" width="100%" height="100%">
					<g:VerticalPanel visible="false" ui:field="otherFeaturesPanel">
						<g:HTML>Other features panel</g:HTML>
						<!-- 
						<g:cell horizontalAlignment="ALIGN_LEFT" width="100%" height="100%">
							<g:DeckLayoutPanel ui:field="otherFeaturesContentContainer" width="100%" height="100%"/>
						</g:cell>
						 -->
					</g:VerticalPanel>
					<!-- 
					<g:VerticalPanel visible="false" ui:field="otherFeaturesPanel">
						<g:HTMLPanel>This is the other features panel that will house the views used to manage services and account specific notifications etc....</g:HTMLPanel>
						<g:Button width="150px" addStyleNames="{style.awsButton}"
							ui:field="closeOtherFeaturesButton">Close</g:Button>
					</g:VerticalPanel>
					 -->
					<g:TabLayoutPanel 
						barHeight="2" 
						barUnit="EM" 
						width="100%"
						height="900px" 
						animationDuration="500" 
						animationVertical="false" 
						ui:field="mainTabPanel"
						addStyleNames="gwt-TabLayoutPanel">
		
						<g:tab>
							<g:header>Home</g:header>
							<g:DeckLayoutPanel ui:field="homeContentContainer" width="100%" height="100%"/>
						</g:tab>
						<g:tab>
							<g:header>AWS Account Maintenance</g:header>
							<g:DeckLayoutPanel ui:field="accountContentContainer" width="100%" height="100%"/>
						</g:tab>
						<!-- 
						<g:tab>
							<g:header>CIDR Assignments</g:header>
							<g:DeckLayoutPanel ui:field="cidrAssignmentContentContainer" width="100%" height="100%"/>
						</g:tab>
						 -->
						<g:tab>
							<g:header>VPC Maintenance</g:header>
							<g:DeckLayoutPanel ui:field="vpcContentContainer" width="100%" height="100%"/>
						</g:tab>
						<g:tab>
							<g:header>VPC Provisioning Maintenance</g:header>
							<g:DeckLayoutPanel ui:field="vpcpContentContainer" width="100%" height="100%"/>
						</g:tab>
						<g:tab>
							<g:header>CIDR Maintenance</g:header>
							<g:DeckLayoutPanel ui:field="cidrContentContainer" width="100%" height="100%"/>
						</g:tab>
						<g:tab>
							<g:header>Elastic IP Maintenance</g:header>
							<g:DeckLayoutPanel ui:field="elasticIpContentContainer" width="100%" height="100%"/>
						</g:tab>
						<!-- 
						<g:tab>
							<g:header>Elastic IP Assignments</g:header>
							<g:DeckLayoutPanel ui:field="elasticIpAssignmentContentContainer" width="100%" height="100%"/>
						</g:tab>
						 -->
						<!-- 
						<g:tab>
							<g:header>Firewall Rule Maintenance</g:header>
							<g:DeckLayoutPanel ui:field="firewallContentContainer" width="100%" height="100%"/>
						</g:tab>
						 -->
						<!-- 
						<g:tab>
							<g:header>e-mail Distribution List Maintenance</g:header>
							<g:HTMLPanel addStyleNames="{style.content}">
								<div>e-mail distribution list Maintentenance content here</div>
							</g:HTMLPanel>
						</g:tab>
						<g:tab>
							<g:header>LDS Maintenance</g:header>
							<g:HTMLPanel addStyleNames="{style.content}">
								<div>LDS Maintentenance content here</div>
							</g:HTMLPanel>
						</g:tab>
						 -->
					</g:TabLayoutPanel>
					</g:VerticalPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_RIGHT" width="%1"><g:HTML></g:HTML></g:cell>
			</g:HorizontalPanel>
			</g:ScrollPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 