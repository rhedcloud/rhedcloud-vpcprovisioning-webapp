<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="DesktopStyles.css" />
	<ui:style field="otherStyles">
	</ui:style>
	<g:ScrollPanel debugId="servicesScrollPanel" width="100%" height="96%">	
		<g:VerticalPanel spacing="12" width="100%" height="100%">
			<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_TOP" width="100%">
				<g:Grid cellSpacing="12">
					<g:row>
				    	<g:customCell>
							<g:Button addStyleNames="{style.glowing-border} {style.normalButton}"
								ui:field="printButton" debugId="printButton">Print</g:Button>
				    	</g:customCell>
				    	<g:customCell>
							<g:Button addStyleNames="{style.glowing-border} {style.normalButton}"
								ui:field="cancelButton" debugId="cancelButton">Close</g:Button>
				    	</g:customCell>
					</g:row>
				</g:Grid>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="100%">
				<g:VerticalPanel ui:field="assessmentPanel">
					<g:HTML addStyleNames="{style.mediumLabel}">Service Security Assessment Report</g:HTML>
					<g:HTML ui:field="assessmentHTML"/>
				</g:VerticalPanel>
			</g:cell>
		</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 