<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="DesktopStyles.css" />
	<ui:style field="otherStyles">
	</ui:style>
	<g:ScrollPanel width="100%" height="99%">
	<g:VerticalPanel spacing="8" width="100%">
		<g:HorizontalPanel ui:field="pleaseWaitPanel" width="100%" visible="false">
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:HorizontalPanel spacing="8">
					<g:Image url="images/ajax-loader.gif"/>
				</g:HorizontalPanel>
			</g:cell>
		</g:HorizontalPanel>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
			<g:Grid cellSpacing="8">
				<g:row>
				    	<g:customCell>
							<g:Label addStyleNames="{style.label}">Account ID:</g:Label>
				    	</g:customCell>
				    	<g:customCell>
							<g:TextBox addStyleNames="{style.field} {style.glowing-border}"
								ui:field="accountIdTB" debugId="accountIdTB" />
				    	</g:customCell>
			    	</g:row>
				<g:row>
				    	<g:customCell>
							<g:Label addStyleNames="{style.label}">Account Name:</g:Label>
				    	</g:customCell>
				    	<g:customCell>
							<g:TextBox addStyleNames="{style.field} {style.glowing-border}"
								ui:field="accountNameTB" debugId="accountNameTB" />
				    	</g:customCell>
			    	</g:row>
				<g:row>
				    	<g:customCell>
							<g:Label addStyleNames="{style.label}">Account Owner:</g:Label>
				    	</g:customCell>
				    	<g:customCell>
							<g:SuggestBox ui:field="ownerIdSB" debugId="ownerIdSB" 
								addStyleNames="{style.field} {style.glowing-border}"/>
				    	</g:customCell>
			    	</g:row>
				<g:row>
			    	<g:customCell>
						<g:Label addStyleNames="{style.label}">Compliance Class:</g:Label>
			    	</g:customCell>
			    	<g:customCell>
						<g:ListBox addStyleNames="{style.field} {style.glowing-border}"
							ui:field="complianceClassLB" debugId="complianceClassLB" />
			    	</g:customCell>
				</g:row>
				<g:row>
				    	<g:customCell>
							<g:Label addStyleNames="{style.label}">Password Location:</g:Label>
				    	</g:customCell>
				    	<g:customCell>
							<g:TextBox addStyleNames="{style.field} {style.glowing-border}"
								ui:field="passwordLocationTB" debugId="passwordLocationTB" />
				    	</g:customCell>
			    	</g:row>
				<g:row>
				    	<g:customCell>
							<g:Label addStyleNames="{style.label}">10-Digit SpeedType:</g:Label>
				    	</g:customCell>
				    	<g:customCell>
							<g:TextBox addStyleNames="{style.field} {style.glowing-border}"
								ui:field="speedTypeTB" debugId="speedTypeTB" />
				    	</g:customCell>
				    	<g:customCell>
							<g:HTML ui:field="speedTypeHTML">SpeedType HTML</g:HTML>
				    	</g:customCell>
			    	</g:row>
			</g:Grid>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="100%">
			<g:VerticalPanel spacing="8" ui:field="sensitiveDataPanel"/>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="100%">
			<g:HorizontalPanel>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="50%">
						<g:ScrollPanel>
							<g:VerticalPanel spacing="8" ui:field="adminVP" height="100%">
								<g:Label addStyleNames="{style.heading_label}">Add/remove Users/Roles:</g:Label>
								<g:HorizontalPanel ui:field="addNetIdHP" spacing="16">
									<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
										<g:SuggestBox ui:field="directoryLookupSB" debugId="directoryLookupSB" addStyleNames="{style.field} {style.glowing-border}"/>
									</g:cell>
									<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
										<g:Button ui:field="addAdminButton" debugId="addAdminButton"
											addStyleNames="{style.glowing-border} {style.actionButton}">Add</g:Button>
									</g:cell>
								</g:HorizontalPanel>
								<g:VerticalPanel spacing="8">
									<g:Label ui:field="adminLabel" addStyleNames="{style.heading_label}">Users/Roles associated to this AWS Account:</g:Label>
									<g:FlexTable ui:field="adminTable" cellSpacing="3" cellPadding="3"/>
								</g:VerticalPanel>
							</g:VerticalPanel>
						</g:ScrollPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="50%">
						<g:ScrollPanel>
							<g:VerticalPanel spacing="8" ui:field="emailsVP" height="100%">
								<g:Label addStyleNames="{style.heading_label}">Add/remove email(s):</g:Label>
								<g:HorizontalPanel ui:field="addEmailHP" spacing="16">
									<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
										<g:TextBox ui:field="addEmailTF" debugId="addEmailTF" addStyleNames="{style.field} {style.glowing-border}"></g:TextBox>
									</g:cell>
									<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
										<g:ListBox addStyleNames="{style.field} {style.glowing-border}"
											ui:field="addEmailTypeLB" debugId="addEmailTypeLB" />
									</g:cell>
									<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
										<g:Button ui:field="addEmailButton" debugId="addEmailButton"
											addStyleNames="{style.glowing-border} {style.actionButton}">Add</g:Button>
									</g:cell>
								</g:HorizontalPanel>
								<g:VerticalPanel spacing="8">
									<g:Label addStyleNames="{style.heading_label}">e-mail addresses associated to this account:</g:Label>
									<g:FlexTable ui:field="emailTable" debugId="emailTable" cellSpacing="3" cellPadding="3"/>
								</g:VerticalPanel>
							</g:VerticalPanel>
						</g:ScrollPanel>
				</g:cell>
			</g:HorizontalPanel>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE" width="100%">
			<g:Grid cellSpacing="12">
				<g:row>
			    	<g:customCell>
						<g:Button addStyleNames="{style.glowing-border} {style.normalButton}"
							ui:field="okayButton" debugId="okayButton">Save</g:Button>
			    	</g:customCell>
			    	<g:customCell>
						<g:Button addStyleNames="{style.glowing-border} {style.normalButton}"
							ui:field="cancelButton" debugId="cancelButton">Cancel</g:Button>
			    	</g:customCell>
			    	<g:customCell>
						<g:Button addStyleNames="{style.glowing-border} {style.actionButton}"
							ui:field="billSummaryButton" debugId="billSummaryButton">View Bills</g:Button>
			    	</g:customCell>
				</g:row>
			</g:Grid>
		</g:cell>
	</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 