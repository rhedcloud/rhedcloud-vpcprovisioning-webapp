<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="DesktopStyles.css" />
	<ui:style field="otherStyles">
	</ui:style>
	<g:ScrollPanel width="100%" height="99%">
	<g:VerticalPanel width="100%" spacing="20">
		<g:HorizontalPanel ui:field="pleaseWaitPanel" width="100%" visible="false">
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:VerticalPanel spacing="8">
					<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:Image url="images/ajax-loader.gif"/>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:HTML ui:field="pleaseWaitHTML" addStyleNames="{style.body}"/>
					</g:cell>
				</g:VerticalPanel>
			</g:cell>
		</g:HorizontalPanel>
		<g:HTML addStyleNames="{style.introHeading}">Virtual Private Cloud Provisioning (VPCP) Console Home</g:HTML>
		<g:HTML ui:field="introBodyHTML" addStyleNames="{style.body}">Welcome, FIRST_NAME, take a look at the information below...</g:HTML>
		 <g:cell>
		 	<g:HTML ui:field="backgroundNoticeHTML" addStyleNames="{style.waitingForWork}">*** Work is 
		 		continuing asynchronously in the background but you can still do work while that finishes up. ***</g:HTML>
		 </g:cell>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
			<g:CaptionPanel addStyleNames="{style.light-captionpanel}" width="90%" captionHTML="Console Features">
				<g:VerticalPanel width="100%" height="100%" spacing="8">
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
						<g:HTML><b>Find Features</b></g:HTML>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
						<g:HTML ui:field="findServicesHTML">You can search for features by their names, key words or acronyms.
						</g:HTML>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
						<g:SuggestBox addStyleNames="{style.longField} {style.glowing-border}"
									ui:field="serviceSearchSB" debugId="serviceSearchSB" />
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
						<g:DisclosurePanel open="true">
							<g:header>Recently used and popular features</g:header>
							<g:VerticalPanel ui:field="recentlyUsedConsoleFeaturesPanel"><g:HTML>This is where the recently visited features will be displayed.</g:HTML></g:VerticalPanel>
						</g:DisclosurePanel>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
						<g:DisclosurePanel ui:field="allConsoleFeaturesDP">
							<g:header>All features</g:header>
							<g:VerticalPanel ui:field="allConsoleFeaturesPanel"><g:HTML>This is where all features will be displayed depending on the user logged in (roles)</g:HTML></g:VerticalPanel>
						</g:DisclosurePanel>
					</g:cell>
				</g:VerticalPanel>
			</g:CaptionPanel>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
		<g:HorizontalPanel width="91.5%" height="100%">
			<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="50%" height="70%">
			<g:CaptionPanel addStyleNames="{style.light-captionpanel}" captionHTML="AWS Information" width="95%" height="90%">
				<g:VerticalPanel width="100%" height="100%" spacing="8">
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP">
						<g:HTMLPanel styleName="{style.body}" ui:field='accountSeriesPanel'>
							<div ui:field='accountSeriesElem'/>
						</g:HTMLPanel>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
						<g:HTML ui:field="emoryAwsInfoHTML" addStyleNames="{style.body}">Click to view a list of your roles.
						</g:HTML>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_BOTTOM">
						<g:Button addStyleNames="{style.glowing-border} {style.normalButton}" width="150px"
							ui:field="roleInfoButton" debugId="roleInfoButton" title="Click to view a list of your roles.">Click To View</g:Button>
					</g:cell>
				</g:VerticalPanel>
			</g:CaptionPanel>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_TOP" width="50%" height="70%">
			<g:CaptionPanel addStyleNames="{style.light-captionpanel}" captionHTML="Directory Information" width="95%" height="90%">
				<g:VerticalPanel width="100%" height="100%" spacing="8">
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP">
						<g:HTML ui:field="directoryInfoHTML" addStyleNames="{style.body}">Click to view the 
						internal directory information used by this application
						</g:HTML>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_BOTTOM">
						<g:Button addStyleNames="{style.glowing-border} {style.normalButton}" width="150px"
							ui:field="directoryInfoButton" debugId="directoryInfoButton" title="Click to view your internal directory information used by this application.">Click To View</g:Button>
					</g:cell>
				</g:VerticalPanel>
			</g:CaptionPanel>
			</g:cell>
		</g:HorizontalPanel>
		</g:cell>
