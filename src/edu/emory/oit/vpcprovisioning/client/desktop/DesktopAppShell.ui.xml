<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="DesktopStyles.css" />
	<ui:style field="otherStyles">
	</ui:style>
	<g:LayoutPanel ui:field='layoutPanel' width="100%" height="100%">
		<!-- header -->
		<g:layer top='0px' height='9.5em' left='0px' right='0px'>
			<g:HorizontalPanel ui:field="linksPanel" addStyleNames="{style.linksPanel}" width="100%" height="100%">
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel spacing="4">
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel spacing="12">
						<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
							<g:HTMLPanel addStyleNames="{style.logo}" ui:field='logoTitleBar' title="Home">
								<div style="padding-left: 4px; padding-right: 4px" ui:field='logoElem'><img src="images/banner-logo-154x45.png"/></div>
							</g:HTMLPanel>
						</g:cell>
						<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_BOTTOM">
							<g:HTMLPanel styleName="{style.featuresMenuHeader}" ui:field='featuresTitleBar' title="Features">
								<div ui:field='featuresElem'>Console Features  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/></div>
							</g:HTMLPanel>
						</g:cell>
					</g:HorizontalPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel styleName="{style.releaseInfoPanel}" ui:field='releaseInfoPanel'>
						<div ui:field='releaseInfoElem'/>
					</g:HTMLPanel>
				</g:cell>
			</g:VerticalPanel>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
			<g:HorizontalPanel ui:field="generalInfoPanel" width="100%">
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTML title="Security Notifications" ui:field='notificationsHTML' styleName="{style.linksMenuHeader}">
						<img src="images/bell-512.png" width="24" height="24"/>
					</g:HTML>
				</g:cell>
				<!-- 
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel styleName="{style.productsMenuHeader}" ui:field='productsTitleBar' title="AWS at Emory">
						<div ui:field='productsElem'>Products  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/></div>
					</g:HTMLPanel>
				</g:cell>
				 -->
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:MenuBar ui:field="productsMenuBar" autoOpen="false" animationEnabled="true" styleName="{style.linksMenu}">
						<g:MenuItem ui:field="productsMenu" debugId="productsMenu" styleName="{style.linksMenuHeader}">Products  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="productsItem" debugId="productsItem" title="Search all products" styleName="{style.linksMenuItem}">Search All Services</g:MenuItem>
								<g:MenuItem ui:field="hipaaServicesItem" debugId="hipaaServicesItem" title="List of services available in HIPAA accounts" styleName="{style.linksMenuItem}">Available HIPAA Account Services</g:MenuItem>
								<g:MenuItem ui:field="standardServicesItem" debugId="standardServicesItem" title="List of services available in Standard accounts" styleName="{style.linksMenuItem}">Available Standard Account Services</g:MenuItem>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:MenuBar ui:field="documentationMenuBar" autoOpen="false" animationEnabled="true" styleName="{style.linksMenu}">
						<g:MenuItem ui:field="documentationMenu" debugId="documentationMenu" styleName="{style.linksMenuHeader}">Documentation  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="emoryKbItem" debugId="emoryKbItem" styleName="{style.linksMenuItem}"></g:MenuItem>
								<g:MenuItem ui:field="awsDocumentationItem" debugId="awsDocumentationItem" title="AWS Documentation" styleName="{style.linksMenuItem}"><a target="_blank" href="https://aws.amazon.com/documentation/">AWS Documentation</a></g:MenuItem>
								<g:MenuItem ui:field="demoItem" debugId="demoItem" styleName="{style.linksMenuItem}"></g:MenuItem>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:MenuBar ui:field="supportMenuBar" autoOpen="false" animationEnabled="true" styleName="{style.linksMenu}">
						<g:MenuItem ui:field="supportMenu" debugId="supportMenu" styleName="{style.linksMenuHeader}">Support  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="contactAwsItem" debugId="contactAwsItem" title="Contact AWS Support" styleName="{style.linksMenuItem}"> Contact AWS Support
									<g:MenuBar vertical="true">
										<g:MenuItem ui:field="awsItem" debugId="awsItem" styleName="{style.linksMenuItem}"></g:MenuItem>
									</g:MenuBar>
								</g:MenuItem>
								<g:MenuItem ui:field="emoryAwsItem" debugId="emoryAwsItem" styleName="{style.linksMenuItem}"></g:MenuItem>
								<g:MenuItem ui:field="esbServiceStatusItem" debugId="esbServiceStatusItem" title="Check System Status" styleName="{style.linksMenuItem}">Check System Status (Central Admins)</g:MenuItem>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:MenuBar ui:field="downloadsMenuBar" autoOpen="false" animationEnabled="true" styleName="{style.linksMenu}">
						<g:MenuItem ui:field="downloadsMenu" styleName="{style.linksMenuHeader}">Downloads  <img src="images/icon-arrow-down-b-512.png" width="16" height="16"/>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="tkiClientItem" debugId="tkiClientItem" title="TKI Client" styleName="{style.linksMenuItem}">TKI Client</g:MenuItem>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:cell>
			</g:HorizontalPanel>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:HorizontalPanel>
				<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel addStyleNames="{style.linksPanelLink}" ui:field='userNameTitleBar'>
						<div ui:field='userNameElem'>[user name]</div>
					</g:HTMLPanel>
				</g:cell>
			</g:HorizontalPanel>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel spacing="4">
						<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
							<g:HTMLPanel addStyleNames="{style.logo}" ui:field='awsLogoTitleBar' title="Home">
								<div style="padding: 6px" ui:field='awsLogoElem'><img src="images/powered-by-aws-white-126x45.png"/></div>
							</g:HTMLPanel>
						</g:cell>
					</g:HorizontalPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel styleName="{style.releaseInfoPanel}">
						<div><p></p></div>
					</g:HTMLPanel>
				</g:cell>
			</g:VerticalPanel>
			</g:cell>
			</g:HorizontalPanel>
		</g:layer>
		
		<!-- Content -->
		<g:layer top='9.5em' bottom='0px' left='0px' right='0px'>
			<g:ScrollPanel width="100%" height="99%">
			<g:VerticalPanel width="100%">
				<g:cell horizontalAlignment="ALIGN_LEFT" width="1%"><g:HTML></g:HTML></g:cell>
				<g:cell horizontalAlignment="ALIGN_CENTER" width="98%">
					<g:VerticalPanel ui:field="appShellPanel" width="100%" height="100%">
					<g:VerticalPanel visible="false" ui:field="otherFeaturesPanel">
						<g:HTML>Other features panel</g:HTML>
					</g:VerticalPanel>
					<g:VerticalPanel ui:field="serviceListPanel" width="100%"/>
					<g:TabLayoutPanel 
						barHeight="0" 
						barUnit="EM" 
						width="100%"
						height="900px" 
						animationDuration="1000" 
						animationVertical="false" 
						ui:field="mainTabPanel"
						addStyleNames="gwt-TabLayoutPanel">
		
						<g:tab>
							<g:header>Home</g:header>
 							<g:DeckLayoutPanel ui:field="homeContentContainer" width="100%"/>
						</g:tab>
					</g:TabLayoutPanel>
					</g:VerticalPanel>
				</g:cell>
				<g:cell horizontalAlignment="ALIGN_RIGHT" width="%1"><g:HTML></g:HTML></g:cell>
			<g:HorizontalPanel spacing="8" width="100%">
				<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:HTMLPanel addStyleNames="{style.rhedcloudLogo}" ui:field='rhedcloudLogo' title="rhedcloud.org">
						<div style="padding-left: 4px; padding-right: 4px" ui:field='rhedcloudLogoElem'><img src="images/rhedcloud-attribution.png"/></div>
					</g:HTMLPanel>
				</g:cell>
			</g:HorizontalPanel>
			</g:VerticalPanel>
			</g:ScrollPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 