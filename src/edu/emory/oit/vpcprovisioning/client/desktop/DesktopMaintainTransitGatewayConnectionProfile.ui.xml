<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="DesktopStyles.css" />
	<ui:style field="otherStyles">
	</ui:style>
	<g:ScrollPanel width="99%" height="99%">
	<g:VerticalPanel spacing="8" width="100%">
		<g:HorizontalPanel ui:field="pleaseWaitPanel" width="100%" visible="false">
			<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:VerticalPanel spacing="8">
					<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:Image url="images/ajax-loader.gif"/>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:HTML ui:field="pleaseWaitHTML" addStyleNames="{style.body}"/>
					</g:cell>
				</g:VerticalPanel>
			</g:cell>
		</g:HorizontalPanel>
		<g:VerticalPanel spacing="12" ui:field="tgwMainPanel">
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
			<g:Label ui:field="maintainTgwcpLabel" addStyleNames="{style.mediumLabel}">Maintain Transit Gateway Connection Profile Metadata</g:Label>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
			<g:Grid addStyleNames="{style.statusPanel}" cellSpacing="8">
				<!-- 
				<g:row>
			    	<g:customCell>
						<g:Label ui:field="maintainTgwcpLabel" addStyleNames="{style.mediumLabel}">Maintain Transit Gateway Connection Profile Metadata</g:Label>
			    	</g:customCell>
				</g:row>
				 -->
				<g:row>
			    	<g:customCell>
						<g:Label ui:field="cidrIdLabel" addStyleNames="{style.label}">CIDR ID:</g:Label>
			    	</g:customCell>
			    	<g:customCell>
						<g:TextBox ui:field="cidrIdTB" debugId="cidrIdTB" 
							addStyleNames="{style.field} {style.glowing-border}"/>
			    	</g:customCell>
		    	</g:row>
				<g:row>
			    	<g:customCell>
						<g:Label ui:field="regionLabel" addStyleNames="{style.label}">Region:</g:Label>
			    	</g:customCell>
			    	<g:customCell>
						<g:ListBox ui:field="regionLB" debugId="regionLB" 
							addStyleNames="{style.field} {style.glowing-border}"/>
			    	</g:customCell>
		    	</g:row>
				<g:row>
			    	<g:customCell>
						<g:Label ui:field="transitGatewayIdLabel" addStyleNames="{style.label}">Transit Gateway ID:</g:Label>
			    	</g:customCell>
			    	<g:customCell>
						<g:ListBox ui:field="transitGatewayIdLB" debugId="transitGatewayIdLB" 
							addStyleNames="{style.field} {style.glowing-border}"/>
			    	</g:customCell>
		    	</g:row>
				<g:row>
			    	<g:customCell>
						<g:Label ui:field="cidrRangeLabel" addStyleNames="{style.label}">CIDR Range:</g:Label>
			    	</g:customCell>
			    	<g:customCell>
						<g:TextBox ui:field="cidrRangeTB" debugId="cidrRangeTB" 
							addStyleNames="{style.field} {style.glowing-border}"/>
			    	</g:customCell>
		    	</g:row>
			</g:Grid>
		</g:cell>
		</g:VerticalPanel>
		<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP" width="100%">
			<g:HorizontalPanel>
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE" width="100%">
					<g:Grid cellSpacing="12">
						<g:row>
					    	<g:customCell>
								<g:Button addStyleNames="{style.glowing-border} {style.normalButton}"
									ui:field="okayButton" debugId="okayButton">Save</g:Button>
					    	</g:customCell>
					    	<g:customCell>
								<g:Button addStyleNames="{style.glowing-border} {style.normalButton}"
									ui:field="cancelButton" debugId="cancelButton">Cancel</g:Button>
					    	</g:customCell>
						</g:row>
					</g:Grid>
				</g:cell>
			</g:HorizontalPanel>
		</g:cell>
	</g:VerticalPanel>
	</g:ScrollPanel>
</ui:UiBinder> 