<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.header {
			background: #1a1a1a;
			color: white;
			font-size: 3.8em;
			line-height: 72pt;
			text-align: center;
			font-weight: bold;
		}

		.subTitle {
			background: #1a1a1a;
			color: white;
			font-size: 14pt;
			text-align: center;
			font-weight: bold;
		}
	
		.content {
			color: black;
			font-size: 14pt;
			text-align: center;
			font-weight: bold;
			line-height: 200%;
		}
	</ui:style>
	<g:LayoutPanel ui:field='layoutPanel' width="100%" height="100%">

		<!-- Title -->
		<g:layer top='0px' height='6.5em' left='0px' right='0px'>
			<g:HTMLPanel ui:field='titleBar' addStyleNames="{style.header}">
				<div ui:field='titleElem'>VPC Provisioning Mobile Web App</div>
			</g:HTMLPanel>
		</g:layer>

		<!-- sub title -->
		<g:layer top='6.5em' height='2.0em' left='0px' right='0px'>
			<g:HTMLPanel ui:field='subTitleBar' addStyleNames="{style.subTitle}">
				<div ui:field='subTitleElem'>Device Not Supported</div>
			</g:HTMLPanel>
		</g:layer>
		
		<!-- Content -->
		<g:layer top='9.5em' bottom='0px' left='0px' right='0px'>
			<g:ScrollPanel width="100%" height="99%">
			<g:HorizontalPanel width="100%">
				<g:cell horizontalAlignment="ALIGN_LEFT" width="1%"><g:HTML></g:HTML></g:cell>
				<g:cell horizontalAlignment="ALIGN_CENTER" width="98%">
					<g:VerticalPanel width="100%" height="100%">
						<g:HTML><h2>The VPCP Console is not currently supported on this device.</h2></g:HTML>
